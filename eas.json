{
  "cli": {
    "version": ">= 13.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://mavehiringserver.azurewebsites.net",
        "EXPO_PUBLIC_API_URL": "https://mavehiringserver.azurewebsites.net/api"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://mavehiringserver.azurewebsites.net",
        "EXPO_PUBLIC_API_URL": "https://mavehiringserver.azurewebsites.net/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
